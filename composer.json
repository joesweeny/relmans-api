{
    "name": "joesweeny/relmans-api",
    "description": "API for Relmans.co.uk",
    "license": "proprietary",
    "type": "project",
    "require": {
        "aws/aws-sdk-php": "^3.173",
        "doctrine/migrations": "^3.0",
        "josegonzalez/dotenv": "^3.2",
        "laminas/laminas-diactoros": "^2.5",
        "league/tactician": "^1.0",
        "monolog/monolog": "^2.2",
        "myclabs/php-enum": "^1.7",
        "paypal/paypal-checkout-sdk": "1.0.1",
        "php-di/php-di": "^6.3",
        "ramsey/uuid": "^4.1",
        "sentry/sdk": "^3.1",
        "sentry/sentry": "^3.1",
        "slim/slim": "4.*",
        "symfony/console": "^5.2"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Relmans\\": "src/app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Relmans\\": "tests/app"
        }
    },
    "config": {
        "sort-packages": true,
        "vendor-dir": "src/vendor",
        "platform": {
            "php": "7.4"
        }
    }
}
